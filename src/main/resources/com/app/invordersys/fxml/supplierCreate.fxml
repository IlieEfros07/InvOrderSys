<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.app.invordersys.controllers.SupplierCreateController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox>
        <TextField fx:id="nameField" promptText="Name" />
        <TextField fx:id="emailField" promptText="Contact Email" />
        <TextField fx:id="phoneField" promptText="Phone" />

        <Button text="Save" onAction="#handleSubmit" />
    </VBox>


</AnchorPane>
